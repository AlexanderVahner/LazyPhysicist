<Window x:Class="ImageCleaner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageCleaner"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <local:Runner/>
    </Window.DataContext>
    <Grid>
        <DockPanel>
            <Button Content="Run" Command="{Binding Run}" DockPanel.Dock="Top"/>
            <RichTextBox x:Name="LogBox" VerticalScrollBarVisibility="Visible"/>
        </DockPanel>
        
    </Grid>
</Window>
